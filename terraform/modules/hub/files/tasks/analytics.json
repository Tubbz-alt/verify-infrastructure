[
  {
    "name": "nginx",
    "image": "${nginx_image_identifier}",
    "cpu": 0,
    "memory": 250,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8443,
        "hostPort": 8443
      }
    ],
    "environment": [
      {
        "Name": "LOCATION_BLOCKS",
        "Value": "${location_blocks_base64}"
      }
    ]
  }
]
