[
  {
    "name": "nginx",
    "image": "${nginx_image_identifier}",
    "cpu": 256,
    "memory": 250,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8443,
        "hostPort": 8443
      }
    ],
    "environment": [
      {
        "Name": "LOCATION_BLOCKS",
        "Value": "${location_blocks_base64}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${deployment}-hub",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "analytics-nginx",
        "awslogs-create-group": "true"
      }
    }
  }
]
