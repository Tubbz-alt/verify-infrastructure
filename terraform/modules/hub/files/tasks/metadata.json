[
  {
    "name": "nginx",
    "image": "${image_and_tag}",
    "cpu": 0,
    "memory": 3500,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8443,
        "hostPort": 8443
      }
    ],
    "environment": [{
      "Name": "DEPLOYMENT",
      "Value": "${deployment}"
    }]
  }
]
